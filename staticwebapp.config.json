{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/widget/*"]
  },
  "responseOverrides": {
    "401": {
      "statusCode": 401,
      "rewrite": "/index.html"
    }
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
